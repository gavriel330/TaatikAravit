{
    "manifest_version": 3,
    "name": "Taatik",
    "version": "1.0",
    "description": "Type in Hebrew and convert to Arabic using the Taatik system.",
    "action": {
      "default_popup": "popup.html",
      "default_title": "Taatik Keyboard"
    },
    "icons": {
      "128": "Taatik128.png"
    },
    "web_accessible_resources": [
      {
        "resources": ["keyboard.js"],
        "matches": ["<all_urls>"]
      }
    ]
  }
  